{
  "expo": {
    "name": "Taxi",
    "slug": "taxi_cliente",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "tech.sinewave.taxi",
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This is for showing the user's location on the map."
      }
    },
    "packagerOpts": {
      "nonPersistent": "--nonPersistent"
    },
    "primaryColor": "#ffc107"
  }
}
